this.AppSvc_P01_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	//초기화 함수
	this.fnInit();
};

//페이지 접근 시 초기 세팅
this.fnInit = function()
{
	var params = this.parent;
	
	var EV_ID = params.EV_ID;
	var EV_TITLE = params.EV_TITLE;
	var YEAR = params.YEAR;
	var MONTH = params.MONTH;
	var SITE_ID = params.SITE_ID;
	var SITE_NM = params.SITE_NM;
	var DEPT_ID = params.DEPT_ID;
	var DEPT_NM = params.DEPT_NM;
	var REG_EMP_NO = params.REG_EMP_NO;
	var REG_USER_NM = params.REG_USER_NM;
	var EV_DESC = params.EV_DESC;
	
	this.div.form.divInfo.form.EditEvId.set_value(EV_ID);				//문서ID
	this.div.form.divInfo.form.EditEvTitle.set_value(EV_TITLE);			//문서 제목
	this.div.form.divInfo.form.EditEvYear.set_value(YEAR);				//등록연도
	this.div.form.divInfo.form.EditEvMonth.set_value(MONTH);			//등록월
	this.div.form.divInfo.form.ComboSite.set_value(SITE_ID);			//등록사업소
	this.div.form.divInfo.form.ComboDept.set_value(DEPT_ID);			//등록부서
	this.div.form.divInfo.form.EditEvYear00.set_value(REG_USER_NM);		//등록자명
	this.div.form.divInfo.form.EditEvEmpNo.set_value(REG_EMP_NO);		//등록자 사번
	this.div.form.divInfo.form.EditEvEmpNo00.set_value(EV_DESC);		//문서요약
	
	this.fnTranTeam();
};		

this.fnTranSave = function()
{
	var strSvcId 	 = "Save";
	var strSvcUrl 	 = "XUP_DEMO::EVD_03";	
	var inData 		 = "dsCond=dsCond";
	var outData 	 = "dsResult1=RESULT0 dsResult2=RESULT1";
	var strArg 		 = "";
	var callBackFunc = "fnCallback";
	var isAsync 	 = true;
	
	this.gfnTransaction(strSvcId,
						strSvcUrl,
						inData,
						outData,
						strArg,
						callBackFunc, 
						isAsync);
}

this.fnTranTeam = function()
{
	var strSvcId 	 = "TeamInfo";
	var strSvcUrl 	 = "XUP_DEMO::APP_03";	
	var inData 		 = "";
	var outData 	 = "dsSite=dsSite dsDept=dsDept dsInsa=dsInsa";
	var strArg 		 = "";
	var callBackFunc = "fnCallback";
	var isAsync 	 = true;
	
	this.gfnTransaction(strSvcId,
						strSvcUrl,
						inData,
						outData,
						strArg,
						callBackFunc, 
						isAsync);
}

this.fnTranSearchInsa = function(EMP_ID)
{
	var strSvcId 	 = "SearchInsa";
	var strSvcUrl 	 = "XUP_DEMO::APP_04";	
	var inData 		 = "";
	var outData 	 = "dsTempInsa=dsTempInsa";
	var strArg 		 = "EMP_ID="+EMP_ID;
	var callBackFunc = "fnCallback";
	var isAsync 	 = true;
	
	this.gfnTransaction(strSvcId,
						strSvcUrl,
						inData,
						outData,
						strArg,
						callBackFunc, 
						isAsync);
}

//트랜잭션 callback
this.fnCallback = function(sId,nErrorCode,sErrorMSG)
{
	if(nErrorCode == 0)
	{
		switch (sId) {
			case "Search":
				
				
				break;
			
			case "TeamInfo":
				var siteRow = this.dsSite.rowcount;
				var deptRow = this.dsDept.rowcount;
				var insaRow = this.dsInsa.rowcount;
				
				for(var i=0; i<siteRow; i++){
					var iRow = this.dsTree.addRow();
					
					var SITE_ID = this.dsSite.getColumn(i, "SITE_ID");
					var SITE_NM = this.dsSite.getColumn(i, "SITE_NM");
					
					this.dsTree.setColumn(iRow, "ID", SITE_ID);
					this.dsTree.setColumn(iRow, "NM", SITE_NM);
					this.dsTree.setColumn(iRow, "LEVEL", 0);
					
					for(var j=0; j<deptRow; j++){
						var UPPO_ID = this.dsDept.getColumn(j, "UPPO_SITE_ID");
						
						if(UPPO_ID == SITE_ID){
							var nRow = this.dsTree.addRow();
							var DEPT_ID = this.dsDept.getColumn(j, "DEPT_ID");
							var DEPT_NM = this.dsDept.getColumn(j, "DEPT_NM");
							
							this.dsTree.setColumn(nRow, "ID", DEPT_ID);
							this.dsTree.setColumn(nRow, "NM", DEPT_NM);
							this.dsTree.setColumn(nRow, "LEVEL", 1);
							
							for(k=0; k<insaRow; k++){
								var insa_DEPT_ID = this.dsInsa.getColumn(k, "DEPT_ID");
								
								if(insa_DEPT_ID == DEPT_ID){
									var kRow = this.dsTree.addRow();
									var EMP_ID = this.dsInsa.getColumn(k, "EMP_ID");
									var EMP_NM = this.dsInsa.getColumn(k, "EMP_NM");
									var POSITION_NM = this.dsInsa.getColumn(k, "POSITION_NM");
									
									var insertInsaInfo = EMP_NM + " " + POSITION_NM + "(" + EMP_ID + ")";
									
									this.dsTree.setColumn(kRow, "ID", EMP_ID);
									this.dsTree.setColumn(kRow, "NM", insertInsaInfo);
									this.dsTree.setColumn(kRow, "LEVEL", 2);
									
								}
							}
						}
					}
				};
				this.fnSetTreeItem();
				break;
					
			case "SearchInsa":
				var rowPos = this.dsTempInsa.rowposition;
				
				var EMP_ID = this.dsTempInsa.getColumn(rowPos, "EMP_ID");
				var DEPT_ID = this.dsTempInsa.getColumn(rowPos, "DEPT_ID");
				var DEPT_NM = this.dsTempInsa.getColumn(rowPos, "DEPT_NM");
				var EMP_NM = this.dsTempInsa.getColumn(rowPos, "EMP_NM");
				var POSITION_CD = this.dsTempInsa.getColumn(rowPos, "POSITION_CD");
				var POSITION_NM = this.dsTempInsa.getColumn(rowPos, "POSITION_NM");
				
				//최종결재자(부장)이 지정 되었다면 지정하지 못하게한다.
				var findRow = this.dsApprove.findRow("POSITION_CD", "P5")
				
				if(findRow != -1){
					alert("최종결재자로 부장을 1명이상 지정할 수 없습니다.");
					return false;
				} else {
					var nRow = this.dsApprove.addRow();
					if(nRow == 0){
						this.dsApprove.setColumn(nRow, "APPROVE_STATUS", "PENDING");
					} else {
						this.dsApprove.setColumn(nRow, "APPROVE_STATUS", "WAITING");
					}
				
					this.dsApprove.setColumn(nRow, "EMP_ID", EMP_ID);
					this.dsApprove.setColumn(nRow, "DEPT_ID", DEPT_ID);
					this.dsApprove.setColumn(nRow, "DEPT_NM", DEPT_NM);
					this.dsApprove.setColumn(nRow, "EMP_NM", EMP_NM);
					this.dsApprove.setColumn(nRow, "POSITION_CD", POSITION_CD);
					this.dsApprove.setColumn(nRow, "POSITION_NM", POSITION_NM);
					
					this.dsTempInsa.clearData();
				}
				
				break;
			
			default:
				
				break;
		}
	}
};

//컴포넌트 이벤트

//조직도 더블클릭
this.div_Grid00_oncelldblclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var nRow = this.dsTree.rowposition;
	
	var ID = this.dsTree.getColumn(nRow, "ID");
	var NM = this.dsTree.getColumn(nRow, "NM");
	var LEVEL = this.dsTree.getColumn(nRow, "LEVEL");
	
	var CHK = this.dsTree.getColumn(nRow, "CHK");
	
	if(LEVEL == 2){
		this.div.form.Grid00.treeusebutton = "no";
	}
	
	if(LEVEL != 2){
		alert("사업소 혹은 부서를 선택 할 순 없습니다.");
	} else if(CHK == 1) {
		alert("이미 선택 되었습니다.");
	} else {
		this.dsTree.setColumn(nRow, "CHK", 1);
		this.fnTranSearchInsa(ID);
	};
};

//저장버튼
this.div_ButtonSave_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	var EV_ID = this.div.form.divInfo.form.EditEvId.value;					//문서번호
	var EV_TITLE = this.div.form.divInfo.form.EditEvTitle.value;			//문서제목
	var EV_DESC = this.div.form.divInfo.form.EditEvEmpNo00.value;	    	//문서요약
	var REG_EMP_NO = this.div.form.divInfo.form.EditEvEmpNo.value;			//등록자 사번
	var REG_USER_NM = this.div.form.divInfo.form.EditEvYear00.value;		//등록자명
	var SITE_ID = this.div.form.divInfo.form.ComboSite.value;				//등록사업소 ID
	var SITE_NM = this.div.form.divInfo.form.ComboSite.text;				//등록사업소 명
	var DEPT_ID = this.div.form.divInfo.form.ComboDept.value;				//등록부서 ID
	var DEPT_NM = this.div.form.divInfo.form.ComboDept.text;				//등록부서 명
	
	var validationList = [
		{ field: "문서번호", value: EV_ID },
		{ field: "문서제목", value: EV_TITLE },
		{ field: "등록자사번", value: REG_EMP_NO },
		{ field: "등록자명", value: REG_USER_NM },
		{ field: "사업소ID", value: SITE_ID },
		{ field: "사업소명", value: SITE_NM },
		{ field: "부서ID", value: DEPT_ID },
		{ field: "부서명", value: DEPT_NM }
	];

	//필수입력값 체크
	var validation = this.validationChk(validationList);
	
	if(validation){
		var nRow = this.dsCond.addRow();
		
		this.dsCond.setColumn(nRow, "EV_ID", EV_ID);
		this.dsCond.setColumn(nRow, "EV_TITLE", EV_TITLE);
		this.dsCond.setColumn(nRow, "EV_DESC", EV_DESC);
		this.dsCond.setColumn(nRow, "REG_EMP_NO", REG_EMP_NO);
		this.dsCond.setColumn(nRow, "REG_USER_NM", REG_USER_NM);
		this.dsCond.setColumn(nRow, "SITE_ID", SITE_ID);
		this.dsCond.setColumn(nRow, "SITE_NM", SITE_NM);
		this.dsCond.setColumn(nRow, "DEPT_ID", DEPT_ID);
		this.dsCond.setColumn(nRow, "DEPT_NM", DEPT_NM);
		
		//저장 트랜잭션 호출
		this.fnTranSave();
	};
};

//취소버튼
this.div_ButtonCancel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var cancelConfirm = this.confirm("등록작업을 취소 하시겠습니까?");
	
	if(cancelConfirm){
		this.close()
	} else {
		return false
	};
};

this.fnSetTreeItem = function()
{
	var rowCnt = this.dsTree.rowcount;
	
	var params = this.parent;
	var REG_EMP_NO = params.REG_EMP_NO;
	
	if(rowCnt > 0){
		//this.dsTree.filter("LEVEL >= 0");
		var nRow = this.dsTree.findRow("ID", REG_EMP_NO);
		this.dsTree.setColumn(nRow, "CHK", 1);
		
		//선택된 로우의 부모
		var uppoRow = this.div.form.Grid00.getTreeParentRow(nRow);
		this.dsTree.setColumn(uppoRow, "CHK", 1);
		
		//선택된 로우의 최상위 부모
		var parentRow = this.div.form.Grid00.getTreeParentRow(uppoRow);
		this.dsTree.setColumn(parentRow, "CHK", 1);
		
		//데이터셋에서 선택된 로우의 값을 가지고 그리드 상의 로우값을 찾는다.
		//찾은 그리드상의 로우 값으로 그리드 tree의 상태값을 변경
		var gridRow = this.div.form.Grid00.getTreeRow(uppoRow);
		this.div.form.Grid00.setTreeStatus(gridRow, true);
		
		//문서를 작성한 사람을 결재 라인의 제일 처음에 올린다.
		this.fnTranSearchInsa(REG_EMP_NO);
		
	} else {
		alert("조직도를 불러 오는 과정에서 문제가 발생 하였습니다.");
	}
	
};]]></Script>
